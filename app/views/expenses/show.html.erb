<p id="notice"><%= notice %></p>

<p>
  <strong>Name:</strong>
  <%= @expense.name %>
</p>

<% if @expense.category %>
  <p>
    <strong>Category:</strong>
    <%= @expense.category.title %>
  </p>
<% else %>
  <p>
    None
  </p>
<% end %>

<p>
  <strong>Amount:</strong>
  <%= @expense.amount %>
</p>

<p>
  <strong>Duedate:</strong>
  <%= @expense.dueDate %>
</p>

<p>
  <strong>Paymentdate:</strong>
  <%= @expense.paymentDate %>
</p>

<p>
  <strong>Latefee:</strong>
  <%= @expense.lateFee %>
</p>

<p>
  <strong>Apr:</strong>
  <%= @expense.apr %>
</p>

<p>
  <strong>Paid:</strong>
  <%= @expense.paid %>
</p>

<%= link_to 'Edit', edit_expense_path(@expense) %> |
<%= link_to 'Back', expenses_path %>
